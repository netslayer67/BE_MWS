{
  "name": "integra-learn-backend",
  "version": "1.0.0",
  "description": "Backend API for IntegraLearn Hub Emotional Wellness Platform",
  "main": "src/server.js",
  "type": "commonjs",
  "scripts": {
    "start": "nodemon src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:mtss": "jest mtss-grade7-helix.test.js --verbose",
    "test:mtss:watch": "jest mtss-grade7-helix.test.js --watch",
    "test:cypress": "cypress run --e2e",
    "test:cypress:mtss": "cypress run --e2e --spec cypress/e2e/mtss-grade7-helix.cy.js",
    "test:cypress:mtss:open": "cypress open --e2e --browser chrome",
    "seed": "node src/scripts/seedUsersFromData.js",
    "seed:mtss": "node src/scripts/seedMtssData.js",
    "seed:mtss-helix": "node src/scripts/seedMtssGrade7HelixComplete.js",
    "import:legacy-checkins": "node src/scripts/importLegacyCheckins.js"
  },
  "keywords": [
    "emotional-wellness",
    "education",
    "ai-psychology",
    "express",
    "mongodb"
  ],
  "author": "IntegraLearn Hub",
  "license": "ISC",
  "dependencies": {
    "@google/genai": "^1.25.0",
    "@google/generative-ai": "^0.24.1",
    "@slack/socket-mode": "^2.0.2",
    "@slack/web-api": "^7.8.0",
    "axios": "^1.13.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.1",
    "multer": "^2.0.2",
    "node-cache": "^5.1.2",
    "nodemailer": "^7.0.10",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "socket.io": "^4.8.1",
    "uuid": "^13.0.0",
    "winston": "^3.18.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "cypress": "^15.7.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4"
  }
}
